{
  "name": "@thenamespace/offchain-manager",
  "version": "1.0.5",
  "description": "TypeScript SDK for creating and managing ENS subnames off-chain with the Namespace API.",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "ens",
    "ethereum",
    "subnames",
    "domains",
    "web3",
    "sdk",
    "offchain",
    "namespace"
  ],
  "files": [
    "/dist"
  ],
  "author": "Arti <arti@namespace.ninja>",
  "contributors": [
    "Harpreet Singh <harpreet@namespace.ninja>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thenamespace/namespacesdk",
    "directory": "packages/offchain-manager"
  },
  "homepage": "https://namespace.ninja",
  "bugs": {
    "url": "https://github.com/thenamespace/namespacesdk/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "restart": "rimraf dist && tsc && node dist/index.js",
    "dev": "ts-node examples/basic-usage.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:manual": "ts-node scripts/test-manual.ts",
    "test:e2e": "ts-node scripts/test-e2e.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test:coverage",
    "clean": "rimraf dist coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^22.10.5",
    "dotenv": "^16.3.1",
    "jest": "^29.5.0",
    "msw": "^2.6.8",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
